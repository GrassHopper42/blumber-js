    "bcrypt": "^5.1.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^4.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "prisma": "^4.10.1",
    "zod": "^3.20.6"
